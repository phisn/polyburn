{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "with-env": "dotenv -e ../../.env.* --",
        "dev": "pnpm with-env wrangler dev src/index.ts --live-reload --port=3002 --var=CF_PAGES_URL:http://localhost:3000 DEV:true",
        "migrate-dev": "dotenv -e ./.env.development -- prisma migrate dev",
        "build": "pnpm esbuild src/index.ts --bundle --format=esm --platform=browser --target=es2020 --outfile=../../dist/_worker.js --minify --legal-comments=none",
        "lint": "eslint \"src/**/*.{tsx,ts}\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20230821.0",
        "dotenv-cli": "^7.3.0",
        "esbuild": "^0.19.2",
        "eslint-config-custom": "workspace:^",
        "prisma": "^5.2.0",
        "typescript": "^5.2.2",
        "wrangler": "^3.6.0"
    },
    "dependencies": {
        "@dimforge/rapier2d": "^0.11.2",
        "@prisma/client": "5.2.0",
        "@trpc/server": "^10.38.1",
        "morgan": "^1.10.0",
        "runtime": "workspace:*",
        "shared": "workspace:*",
        "tsconfig": "workspace:*",
        "zod": "^3.22.2"
    }
